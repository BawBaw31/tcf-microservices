{
  "baseUrl": "/products",
  "endpoint": "PRODUCT_URL",
  "middlewares": ["verifyToken"],
  "keepBaseUrl": true,
  "routes": [
    {
      "method": "GET",
      "path": "/",
      "middlewares": []
    },
    {
      "method": "POST",
      "path": "/"
    },
    {
      "method": "GET",
      "path": "/:id",
      "middlewares": []
    },
    {
      "method": "PUT",
      "path": "/:id"
    },
    {
      "method": "DELETE",
      "path": "/:id"
    }
  ],
  "translations": {
    "product": "/products/:id"
  }
}
